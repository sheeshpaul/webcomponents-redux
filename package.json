{
  "name": "webcomponents-redux",
  "version": "0.0.2",
  "description": "Redux binding for web component",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sheeshpaul/webcomponents-redux.git"
  },
  "keywords": [
    "webcomponents-redux",
    "connect"
  ],
  "author": "Sheeshpaul Kamboj <sheeshpaul@hotmail.com>",
  "main": "lib/webcomponents-redux.js",
  "unpkg": "dist/webcomponents-redux.js",
  "module": "es/webcomponents-redux.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types"
  ],
  "scripts": {
    "clean": "rimraf lib dist es types",
    "check-types": "tsc --noEmit",
    "lint": "eslint --ext js,ts src",
    "prepare": "npm run clean && npm run check-types && npm run lint && npm run build",
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "rollup": "^1.30.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify-es": "0.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.8.2"
  },
  "npmName": "webcomponents-redux",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "sideEffects": false
}
